{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "User pilih payment method",
  "required": [
    "rescode","message","data"
  ],
  "properties": {
    "rescode": {
      "type": "string"
    },
    "message": {
      "type": "object",
      "required": [
        "title","body"
      ],
      "properties": {
        "title": {
          "type": "string"
        },
        "body": {
          "type": "string"
        }
      }
    },
    "data": {
      "type": "object",
      "required": [
        "order_id"
        ],
      "properties": {
        "order_id": {
          "type": "string"
        }
      }
    }
  }
}